<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>javadoc</actionName>
            <packagings>
                <packaging>*</packaging>
            </packagings>
            <goals>
                <goal>generate-sources</goal>
                <goal>javadoc:javadoc</goal>
